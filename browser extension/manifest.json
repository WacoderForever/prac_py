{
    "manifest_version": 3,
    "name": "YouTube Downloader",
    "description": "This extension sends the current YouTube video URL to a local server for downloading.",
    "version": "1.0",
    "permissions": ["activeTab"],
    "background": {
      "service_worker": "background.js"
    },
    "action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "",
        "48": "images/icon48.png",
        "128": "images/icon128.png"
      }
    }
  }
  